doctype 5
html(lang="en")
  head
    meta(charset="utf-8")
    title turing
    != css('styles.css')

  body(data-view="#{view}")
    block content

    | <script>
    | document.write('<script src=' +
    | ('__proto__' in {} ? 'http://zeptojs.com/zepto.min.js' : 'http://code.jquery.com/jquery-1.7.2.min.js') +
    | '><\/script>')
    | </script>

    block scripts
        != js("//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js")
        != js('scripts.js')
